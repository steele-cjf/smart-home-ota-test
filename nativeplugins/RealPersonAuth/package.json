{  
    "name": "RealPersonAuth",  
    "id": "RealPersonAuth",  
    "version": "4.0.4",  
    "description": "实人认证插件",  
    "_dp_type":"nativeplugin",  
    "_dp_nativeplugin":{
        "android": {
            "plugins": [
                {
                    "type": "module",  
                    "name": "RealPersonAuth",
                    "class": "com.alibaba.security.cloud.CloudRealIdentityTrigger"  
                }
            ],  
            "hooksClass": "CloudRealIdentityTrigger",  
            "integrateType": "aar", 
            "permissions": [
				"android.permission.CAMERA",
				"android.permission.WRITE_EXTERNAL_STORAGE",
				"android.permission.READ_EXTERNAL_STORAGE"
            ]
        }
    }  
}